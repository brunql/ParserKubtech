 var isIE5 = (document.all) && (navigator.appVersion.indexOf("5.")!=-1 || navigator.appVersion.indexOf("6.") != -1);
 var STime = new Date();
 var GMToff = -(STime.getTimezoneOffset()/60);
 if (GMToff >= 0) {GMToff = '+' + GMToff};
 function GetNDate(oTime) {
   var d1 = oTime.getDate();
   var m1 = oTime.getMonth() + 1;
   var h1 = oTime.getHours();
   var i1 = oTime.getMinutes();
   var s1 = oTime.getSeconds();
   if (d1 <10) {d1 = "0" + d1};
   if (m1 <10) {m1 = "0" + m1};
   if (h1 <10) {h1 = "0" + h1};
   if (i1 <10) {i1 = "0" + i1};
   if (s1 <10) {s1 = "0" + s1};
   return d1 + '.' + m1 + '.' + oTime.getYear() + ' ' + h1 + ':' + i1 + ':' + s1;
 }
 function TimeBarRefresh() {
  if (isIE5) {
   var LTime = new Date();
   var RVUTime = new Date("June 28, 2010 18:59:24");
   RVUTime.setMilliseconds(LTime - STime);
   document.all.TimeBar.innerHTML = '&nbsp;Ваше время: ' + GetNDate(LTime) + ' (GMT' + GMToff + '), в ВП сейчас: ' + GetNDate(RVUTime) + ' (GMT+3)';
   setTimeout('TimeBarRefresh()', 1000);
  }
 }
 TimeBarRefresh();
