 if (GMToff >= 0) {GMToff = '+' + GMToff};
 function GetNDate(oTime) {
   var d1 = oTime.getDate();
   var m1 = oTime.getMonth() + 1;
   var h1 = oTime.getHours();
   var i1 = oTime.getMinutes();
   var s1 = oTime.getSeconds();
   if (d1 <10) {d1 = "0" + d1};
   if (m1 <10) {m1 = "0" + m1};
   if (h1 <10) {h1 = "0" + h1};
